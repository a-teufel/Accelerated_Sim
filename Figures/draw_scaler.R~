library(ggplot2)
library(cowplot)
library(plyr)
library(reshape2)
require(vcd)
require(MASS)


cov_100=1.5375
cov_50=1.35
cov_25=.99

y_cov<-c(cov_100,cov_50,cov_25)
x<-c(100,50,25)

y<-c(100/1.5375,50/1.35,25/.99)

plot(x,y,xlim=c(0,100),ylim=c(0,100))


temp<-data.frame(x=x,y=y)



last<-ggplot(data=temp, aes(x=x, y=y)) +geom_point()+xlab("Population Size\nOriginal Sampling")+ylab("Converted Population Size\nAccelerated Sampling")+xlim(0,100)+ylim(0,100)+geom_smooth(method="lm",se=FALSE,colour="red",fullrange=TRUE)

fit_sim<-nls(y~m*x+b,start=list(m=1,b=0))

smooth_vals = lm(y~x)
print(smooth_vals)

save_plot("convertN.pdf",last,base_aspect_ratio=1.1)
